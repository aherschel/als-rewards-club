{
  "name": "als-rewards-club-backend",
  "version": "0.1.0",
  "bin": {
    "backend": "lib/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "deploy:secrets": "npx aws-cdk deploy AlsRewardsClubSecrets",
    "deploy:hosting": "npx aws-cdk deploy AlsRewardsClubHosting",
    "deploy:dev": "npx aws-cdk deploy AlsRewardsClubBackend --outputs-file ../output.json",
    "deploy": "npx aws-cdk deploy --outputs-file ../output.json",
    "deploy:ci": "npx aws-cdk deploy AlsRewardsClubBackend --require-approval never --outputs-file ../output.json"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.7.1",
    "aws-cdk": "2.103.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "@aws-amplify/auth-construct-alpha": "^0.2.4",
    "@aws-amplify/graphql-api-construct": "^1.1.4",
    "@aws-appsync/utils": "^1.5.0",
    "@aws-cdk/aws-amplify-alpha": "^2.103.0-alpha.0",
    "aws-cdk-lib": "2.103.0",
    "constructs": "^10.3.0",
    "source-map-support": "^0.5.21"
  }
}
